{
  "name": "todomvc",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint --fix \"{**/*,*}.{ts,tsx,js,jsx}\"",
    "format": "prettier --write \"{**/*,*}.{ts,tsx,js,jsx}\"",
    "prepare": "husky install"
