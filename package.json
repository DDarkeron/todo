    "autoprefixer": "^10.3.4",
    "eslint": "^7.32.0",
    "eslint-config-alloy": "^4.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.4.0",
    "prettier-plugin-organize-imports": "^2.3.3",
    "tailwindcss": "^2.2.15",
    "typescript": "^4.4.3",
    "vite": "^2.5.7"
  }
